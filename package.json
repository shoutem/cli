{
  "name": "@shoutem/cli",
  "version": "0.12.0-alpha12",
  "description": "Command-line tools for Shoutem applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/shoutem/cli"
  },
  "bin": {
    "shoutem": "./build/shoutem.js"
  },
  "scripts": {
    "extlint": "eslint",
    "lint": "eslint ./ --ignore-pattern build --ignore-pattern templates",
    "test": "mocha -R spec --require fetch-everywhere --compilers js:babel-core/register \"src/**/*spec.js\"",
    "shoutem": "node build/shoutem.js",
    "dev": "node src/shoutem.js",
    "prepublish": "dos2unix src/shoutem.js",
    "beta": "npm publish --access public --tag beta",
    "hotfix": "npm publish --access public --tag hotfix",
    "build": "babel ./src --ignore ./src/templates --out-dir build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shoutem/fetch-token-intercept": "0.3.0",
    "@shoutem/fork-terminal": "1.0.0",
    "archiver": "^3.0.0",
    "async": "2.6.0",
    "babel-register": "6.26.0",
    "bluebird": "3.5.1",
    "child-process-promise": "2.2.1",
    "cli-spinner": "0.2.8",
    "colors": "1.2.4",
    "command-exists": "1.0.2",
    "decamelize": "^1.2.0",
    "decompress": "4.0.0",
    "depcheck": "^0.6.9",
    "diff": "^3.3.1",
    "download-cached": "1.0.8",
    "download-file": "latest",
    "eslint": "4.19.1",
    "eslint-plugin-react-native": "3.2.1",
    "exit-code": "1.0.2",
    "extract-zip": "1.6.7",
    "fetch-everywhere": "1.0.5",
    "form-data": "2.1.4",
    "fs": "0.0.1-security",
    "fs-extra": "6.0.1",
    "glob-move": "1.0.1",
    "glob-promise": "3.1.0",
    "ignore": "^5.0.2",
    "inquirer": "5.2.0",
    "ip": "1.1.5",
    "is-gzip": "^2.0.0",
    "json-stringify-safe": "5.0.1",
    "jsonapi-serializer": "3.5.3",
    "linklocal": "^2.8.2",
    "lodash": "4.17.11",
    "lodash-get-or-set": "^1.0.2",
    "mkdirp": "0.5.1",
    "mkdirp-promise": "5.0.1",
    "mustache": "2.3.0",
    "mv": "2.1.1",
    "ngrok": "3.0.1",
    "opn": "^5.3.0",
    "prettyjson": "1.2.1",
    "progress": "2.0.0",
    "request": "2.88.0",
    "qrcode-terminal": "0.12.0",
    "replace-in-file": "3.4.0",
    "request-promise": "4.2.2",
    "request-promise-native": "1.0.5",
    "rmfr": "2.0.0",
    "semver": "5.5.0",
    "slugify": "^1.2.9",
    "streamsearch": "0.1.2",
    "tar": "^4.4.2",
    "tmp-promise": "1.0.4",
    "universal-analytics": "0.4.16",
    "uppercamelcase": "^3.0.0",
    "urijs": "1.19.1",
    "uuid": "3.2.1",
    "yargonaut": "1.1.2",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-plugin-rewire": "1.1.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.2",
    "dos2unix-cli": "1.0.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-react": "7.7.0",
    "mocha": "5.1.1",
    "nock": "9.2.5",
    "sinon": "5.0.7"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "ignore": false,
    "only": [
      "config",
      "src"
    ]
  }
}
